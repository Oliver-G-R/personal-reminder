{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@context/*": [
        "./src/context/*"
      ],
      "@Types/*": [
        "./src/Types/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@helpers/*": [
        "./src/helpers/*"
      ]
    }
  }
}
